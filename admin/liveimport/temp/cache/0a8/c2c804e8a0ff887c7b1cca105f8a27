  $(document).ready(function() {
    var owl123487549 = $(".box #myCarousel123487549 .carousel-inner");
  	
    $("#myCarousel123487549_next").click(function(){
        owl123487549.trigger('owl.next');
        return false;
      })
    $("#myCarousel123487549_prev").click(function(){
        owl123487549.trigger('owl.prev');
        return false;
    });
      
    owl123487549.owlCarousel({
    	  slideSpeed : 500,
        singleItem:true
     });
  });
  </script>
  	
