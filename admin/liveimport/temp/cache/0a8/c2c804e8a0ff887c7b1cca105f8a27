$(document).ready(function() {
  var owl34689690 = $(".box #myCarousel34689690 .carousel-inner");
	
  $("#myCarousel34689690_next").click(function(){
      owl34689690.trigger('owl.next');
      return false;
    })
  $("#myCarousel34689690_prev").click(function(){
      owl34689690.trigger('owl.prev');
      return false;
  });
    
  owl34689690.owlCarousel({
  	  slideSpeed : 500,
      singleItem:true
   });
});
</script>
