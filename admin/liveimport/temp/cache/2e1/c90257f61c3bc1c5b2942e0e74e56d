					<script>
						$(document).ready(function() {

							console.log($("#in_stock").text());
							if ($("#in_stock").text() > 0) {
								$("#product").append('<button class="btn btn-primary btn-lg btn-block order"  type="button" data-toggle="modal" data-target="#order">Быстрый заказ </button>');									
							} else {
								$("#product").append('<button class="btn btn-primary btn-lg btn-block order"  type="button" data-toggle="modal" data-target="#order">Быстрый заказ</button>');									
							}		

							$(".btn.order").click(function(){
								$("#order").find("h2.modal-title").text($("h1").text());
								$("#order").find('input[name="product"]').val("<a href='" + window.location.href + "'>" + $("h1").html() + "</a>");
								console.log($("h1").html());
							});		

						});
					</script>
				            
